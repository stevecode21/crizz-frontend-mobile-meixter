{
  "expo": {
    "name": "Meixter App",
    "slug": "meixter-app",
    "description": "App Meixter develop for Crizz.",
    "privacy": "unlisted",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7815010412581785~4857116020"
      },
      "bundleIdentifier": "com.meixter.app"
    },
    "android": {
      "userInterfaceStyle": "dark",
      "package": "com.meixter.app",
      "versionCode": 1,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7815010412581785~4857116020"
      }
    },
    "androidNavigationBar": {
      "barStyle": "light-content",
      "backgroundColor": "#000000"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/favicon.png",
      "color": "#00FEFF",
      "androidMode": "default",
      "androidCollapsedTitle": "Notifications"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    }
  }
}
